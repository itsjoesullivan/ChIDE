{
  "name": "ChIDE",
  "description": "An IDE for ChromeOS :)",
  "version": "0.8",
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "permissions": [
    {
      "fileSystem": [
        "write"
      ]
    },
      "storage",
      "unlimitedStorage",
      "clipboardRead",
      "clipboardWrite",
      "notifications"
  ],
  "icons": { 
    "256": "images/chide-256.png",
    "128": "images/chide-128.png",
    "16": "images/chide-16.png" 
  }
}
